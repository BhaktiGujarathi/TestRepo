virtual public class ParentCls {
string brand;
   long noofreleases;
    public ParentCls()
    {
        this.brand='Audi';
        this.noofreleases= 100000;
    }
    public void validate(string a,long x)
    {
      brand=a;
      noofreleases=x;
        system.debug('Company :'+this.brand +'\nRelease :' +this.noofreleases);
}
}